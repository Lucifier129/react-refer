  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lucifier129/react-refer/issues"
  },
  "homepage": "https://github.com/Lucifier129/react-refer#readme",
  "devDependencies": {
    "babel": "^5.5.8",
    "babel-core": "^5.6.18",
    "babel-eslint": "^3.1.15",
    "babel-loader": "^5.1.4",
    "expect": "^1.8.0",
    "mocha": "^2.2.5",
    "webpack": "^1.9.6",
    "webpack-dev-server": "^1.8.2"
  },
  "dependencies": {
    "react": "^0.13.3"
  }
